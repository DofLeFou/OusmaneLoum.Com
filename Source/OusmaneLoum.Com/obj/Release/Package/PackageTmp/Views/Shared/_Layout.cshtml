    @Styles.Render("~/Content/BootStrap")
    @Styles.Render("~/Content/FontAwesome")
    @Styles.Render("~/Content/MagnificPopup")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

